{
  "name": "siraj-educational-ai-frontend",
  "version": "8.1.0",
  "description": "Multi-voice educational AI platform frontend - consciousness-driven learning through council methodology",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.263.1",
    "axios": "^1.6.0",
    "web-vitals": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "serve": "npx serve -s build -l 3000",
    "deploy:staging": "npm run build && npm run deploy:staging:upload",
    "deploy:production": "npm run build && npm run deploy:production:upload"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "cypress": "^12.17.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "proxy": "http://localhost:8000",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "education",
    "AI",
    "consciousness-driven",
    "council-methodology",
    "SIRAJ",
    "multi-voice",
    "learning",
    "recursive",
    "QWAN"
  ],
  "author": "SIRAJ Educational AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siraj-ai/educational-platform.git"
  },
  "bugs": {
    "url": "https://github.com/siraj-ai/educational-platform/issues"
  }
}